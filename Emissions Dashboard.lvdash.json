{
  "datasets": [
    {
      "name": "6f03f466",
      "displayName": "Location Data",
      "queryLines": [
        "SELECT \r\n",
        "latitude,\r\n",
        "longitude,\r\n",
        "`GHG emissions mtons CO2e`\r\n",
        "FROM emissions.default.emissions_data;"
      ]
    },
    {
      "name": "3ad9151e",
      "displayName": "Emission per person",
      "queryLines": [
        "SELECT \r\n",
        "  county_state_name,\r\n",
        "  population,\r\n",
        "  CAST(\r\n",
        "    REPLACE(`GHG emissions mtons CO2e`, ',', '') AS DOUBLE\r\n",
        "  ) / CAST(\r\n",
        "    REPLACE(population, ',', '') AS DOUBLE\r\n",
        "  ) AS emission_per_person\r\n",
        "FROM emissions.default.emissions_data\r\n",
        "ORDER BY emission_per_person DESC;"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "811da160",
      "displayName": "total_emission_per_state",
      "queryLines": [
        "SELECT \r\n",
        "  state_abbr,\r\n",
        "  SUM(\r\n",
        "    CAST(\r\n",
        "    REPLACE(`GHG emissions mtons CO2e`, ',', '') AS DOUBLE\r\n",
        "  )) AS total_emission\r\n",
        "FROM emissions.default.emissions_data\r\n",
        "GROUP BY state_abbr\r\n",
        "ORDER BY total_emission DESC\r\n",
        "LIMIT 10;"
      ]
    },
    {
      "name": "37eaad80",
      "displayName": "Couty Shaming",
      "queryLines": [
        "SELECT \r\n",
        "  county_state_name,\r\n",
        "  population,\r\n",
        "    CAST(\r\n",
        "    REPLACE(`GHG emissions mtons CO2e`, ',', '') AS DOUBLE\r\n",
        "  ) AS total_emission\r\n",
        "FROM emissions.default.emissions_data\r\n",
        "ORDER BY total_emission DESC\r\n",
        "LIMIT 10;"
      ]
    }
  ],
